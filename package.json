{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run prepare-deployment && vite build && npm run build:server",
    "build:frontend": "vite build",
    "build:server": "tsc --project tsconfig.server.json --outDir dist && cp dist/src/api/server.js dist/server.js && mkdir -p dist/db && cp -r dist/src/db/* dist/db/ 2>/dev/null || true && mkdir -p dist/types && cp -r dist/src/types/* dist/types/ 2>/dev/null || true && sed -i \"s|from '../db/|from './db/|g\" dist/server.js",
    "prepare-deployment": "node scripts/prepare-deployment.cjs",
    "preview": "vite preview",
    "server": "tsx src/api/server.ts",
    "server:watch": "tsx watch src/api/server.ts",
    "dev:all": "concurrently \"npm run server:watch\" \"npm run dev\"",
    "start": "node dist/server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.5",
    "@types/three": "^0.179.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "pg": "^8.16.3",
    "postcss-scss": "^4.0.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-swipeable": "^7.0.2",
    "recharts": "^3.1.2",
    "three": "^0.179.1",
    "three-stdlib": "^2.36.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/express": "^5.0.3",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.5.6",
    "sass": "^1.90.0",
    "sass-embedded": "^1.90.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.19"
  }
}
